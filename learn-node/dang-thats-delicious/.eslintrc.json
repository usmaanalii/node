{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 8
    },
    "rules": {
        "no-console": 0,
        "no-unused-vars": 0,
        "maximumLineLength": 0,
        "indent": [
            "error",
            2
        ]
    }
}